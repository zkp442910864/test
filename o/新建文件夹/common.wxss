.ellipsis {
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.ellipsis-2 {
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.over-hidden {
    overflow: hidden;
}

.clearfix {
    zoom: 1;
}

.clearfix::after {
    content: "";
    display: block;
    clear: both;
}

.mask2 {
    position: fixed;
    z-index: 999;
    background: rgba(0, 0, 0, .5);
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.mask2-box {
    width: 90%;
    background: #fff;
    left: 50%;
    top: 50%;
    margin-left: -45%;
    transform: translateY(-50%);
    text-align: center;
    border-radius: 12rpx;
    overflow: hidden;
    position: absolute;
}

.mask2 image {
    width: 240rpx;
    height: 237rpx;
    margin: 46rpx auto;
    display: block;
}

.mask2 .title {
    padding: 48rpx 0;
}


.mask2 .cha {
    font-size: 70rpx;
    right: 15rpx;
    top: 15rpx;
}

.mask2 .btn {
    height: 88rpx;
    line-height: 88rpx;
    background: rgba(255, 110, 21, 1);
    border-radius: 44rpx;
    width: 80%;
    margin: 60rpx auto 40rpx;
    color: #fff;
}

.falls {
    column-count: 2;
    column-gap: 20rpx;
}

.falls>view {
    break-inside: avoid;
}